{
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "prettier/prettier": "error",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "next" }],
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  },
  "env": {
    "jest": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"]
}
